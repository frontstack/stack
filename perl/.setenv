#!/bin/bash

PACKAGEPATH=$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )

#### Perl ####
PERL5LIB="$PACKAGEPATH/lib/5.16.3:$PACKAGEPATH/lib/site_perl/5.16.3:$PACKAGEPATH/lib/5.16.3/x86_64-linux-thread-multi:$PACKAGEPATH/lib/site_perl/5.16.3/x86_64-linux-thread-multi:$PACKAGEPATH/lib/5.16.3/x86_64-linux-thread-multi/CORE"
export PERL5LIB

LD_LIBRARY_PATH="$PERL5LIB:$LD_LIBRARY_PATH"
export LD_LIBRARY_PATH
